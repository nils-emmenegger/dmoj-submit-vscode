{
  "name": "dmoj-submit-vscode",
  "displayName": "dmoj-submit-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dmoj-submit-vscode.submit",
        "title": "Submit current file to DMOJ"
      }
    ],
    "configuration": {
      "title": "dmoj-submit-vscode",
      "properties": {
        "dmoj-submit-vscode.command": {
          "type": "string",
          "default": "dmoj-submit",
          "markdownDescription": "dmoj-submit-vscode submits files by sending `${command} submit '${fileName}'` to a terminal. This setting lets you change what `command` is. This can be necessary if, for example, your `dmoj-submit` executable is not in your `PATH` in which case you might replace it with something like `windows` for Windows, `/Users/<user>/Downloads/dmoj-submit` for MacOS, or `/home/<user>/Downloads/dmoj-submit` for Linux."
        }
      }
    },
    "keybindings": [
      {
        "command": "dmoj-submit-vscode.submit",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
